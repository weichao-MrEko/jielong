/* pages/shedidian/shedidian.wxss */
page{
  background: #f4f4f4;
}
.new{
  height: 80rpx;
  background: #fff;
  line-height: 80rpx;
  color: #999;
}
.box{
  display: flex;
  background: #fff;
  margin-top: 20rpx;
  border-bottom: 1rpx solid #f4f4f4;
  padding: 10rpx;
}
.tuimg{
  flex: 0.12
}
.diname{
  flex: 0.85;
  border-left: 2px solid #999;
}
.diname view{
  margin-left: 20rpx;
}
.tuimg image{
  width: 100%;
  height: 100%
}
.del{
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  border-radius: 100%;
  background: #666;
  text-align: center;
  margin-top: 30rpx;
  font-size: 40rpx;
}
.btn{
  width: 100%;
  height: 100rpx;
  background: #f34534;
  line-height:100rpx;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0; 
  color: #fff;
}