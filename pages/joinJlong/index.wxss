.contain{
padding: 10rpx;
justify-content:center;
align-items:center;
flex-direction: column;
width:90%;
height: auto;
margin:auto;
font-size: 26rpx;
}
.onebox{
  display: -webkit-flex; /* Safari */
  display: flex;
  justify-content: center;
  align-items:center;
  align-content: center;
  margin-bottom: 40rpx;
  padding: 10rpx;
}

.bg{
  background-color: #eff1f2;
}
.one_img{
  width:50rpx;height:50rpx;
}
.one_v1{
  width:84%;
}
.one_v2{
  font-size:23rpx; color:#c4c4c4;
}
.one_img2{
  width:40rpx;height:80rpx;
}
.twosty{
  padding: 10rpx;
}
.twobox{
    display: -webkit-flex; /* Safari */
  display: flex;
  justify-content: space-between;
}
.tb_v2{
    display: -webkit-flex; /* Safari */
  display: inline-flex;
}
.two_v1{
  margin-top:40rpx;margin-bottom:40rpx;
}
.two_v1_v1{
  margin-bottom:10rpx;font-weight: bold;font-size:30rpx;
}
.two_v1_v2{
  border:1rpx solid #c4c4c4;padding:10rpx;
}
.two_v1_v2_v1{
  margin-bottom:10rpx;
}
.tb_v1{
  display:inline-flex;color:red;font-weight:bold;
}
.tb_v3{
  text-align:center;width:50rpx;line-height:50rpx;font-size:35rpx;border:1rpx solid #c4c4c4;border-radius:50%;
}
.tb_v4{
  text-align:center;width:50rpx;line-height:50rpx;margin-left:15rpx;margin-right:15rpx;
}
.tb_v5{
  background:#06de2f;color:white;line-height:50rpx; text-align:center;width:50rpx;font-size:35rpx;border-radius:50%;border:1rpx solid #c4c4c4;
}
.three_v1{
  margin-bottom:10rpx;
}
.three_v2_v1{
  font-weight:bold;font-size:30rpx;margin-bottom:15rpx;
}
.three_v2_v1 span{
font-size:26rpx; color:red;
}
.three_v2_v2{
  color:#567195;
}
.three_v2_v2 image{
width:28rpx;height:28rpx;
}
.three_v3{
  min-height: 150rpx;padding:10rpx;
}
.totop{
max-width: 100%;
}
.address{
  padding: 10rpx;
  border-bottom: 1rpx solid #c4c4c4;
  margin-bottom: 10rpx;
}
.ad_v1{
  margin-bottom: 10rpx;
}
.ad_v2{
  color:#c4c4c4;
}
.ad2_img{
  width: 100rpx;height: 100rpx;display:inline-block;line-height: 100rpx;
}

.ad2 view{
  display: flex;
  flex-direction: row;
  border-bottom:1rpx solid #c4c4c4; 
}
.ad2 text{
  display: inline-block;
  width:130rpx;
  height: 100rpx;
  line-height: 100rpx;
}
.ad2 input{
  display: inline-block;
  height: 100rpx;
  min-width: 68%;
}
.foot{
  background-color: #06de2f;
  height: 50rpx;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  line-height: 50rpx;
  text-align: center;
}

.foot span{
  color:white;
  font-size: 40rpx;
  font-weight: bold;
}